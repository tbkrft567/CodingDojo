<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    @{
        List<Dictionary<string, object>> rentals = ViewBag.Rentals;
    }

    @{
        @foreach(var item in rentals){
            <p>Rental_ID: @item["rental_id"]</p>
            <p>Inventory_ID: @item["inventory_id"]</p>
        }
    }

</body>
</html>