<!DOCTYPE html>
<html>
  <head>
    <title>DojoSecrets2</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
    <a href="/sessions/logout" data-method="delete">Log Out</a>
    <% end %>
    <%= yield %>

  </body>
</html>
